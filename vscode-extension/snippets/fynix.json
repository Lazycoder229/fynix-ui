{
  "Fynix Component (TypeScript)": {
    "prefix": "fnx-ts",
    "body": [
      "<logic setup=\"ts\">",
      "import { nixState } from \"@fynixorg/ui\";",
      "",
      "$1",
      "</logic>",
      "",
      "<view>",
      "  <div>",
      "    $2",
      "  </div>",
      "</view>",
      "",
      "<style scoped>",
      "div {",
      "  $3",
      "}",
      "</style>"
    ],
    "description": "Create a Fynix SFC component with TypeScript"
  },
  "Fynix Component (JavaScript)": {
    "prefix": "fnx-js",
    "body": [
      "<logic setup=\"js\">",
      "import { nixState } from \"@fynixorg/ui\";",
      "",
      "$1",
      "</logic>",
      "",
      "<view>",
      "  <div>",
      "    $2",
      "  </div>",
      "</view>",
      "",
      "<style scoped>",
      "div {",
      "  $3",
      "}",
      "</style>"
    ],
    "description": "Create a Fynix SFC component with JavaScript"
  },
  "Fynix Component with State": {
    "prefix": "fnx-state",
    "body": [
      "<logic setup=\"ts\">",
      "import { nixState } from \"@fynixorg/ui\";",
      "",
      "const ${1:count} = nixState<${2:number}>(${3:0});",
      "</logic>",
      "",
      "<view>",
      "  <div>",
      "    <h1>{${1:count}.value}</h1>",
      "    <button r-click={() => ${1:count}.value++}>Increment</button>",
      "  </div>",
      "</view>",
      "",
      "<style scoped>",
      "div {",
      "  display: flex;",
      "  flex-direction: column;",
      "  gap: 1rem;",
      "}",
      "</style>"
    ],
    "description": "Create a Fynix component with state"
  },
  "Logic Block": {
    "prefix": "logic",
    "body": ["<logic setup=\"${1|ts,js|}\">", "$2", "</logic>"],
    "description": "Create a logic block"
  },
  "View Block": {
    "prefix": "view",
    "body": ["<view>", "  $1", "</view>"],
    "description": "Create a view block"
  },
  "Style Block": {
    "prefix": "style",
    "body": ["<style${1| , scoped|}>", "$2", "</style>"],
    "description": "Create a style block"
  },
  "nixState": {
    "prefix": "nstate",
    "body": ["const ${1:state} = nixState<${2:type}>(${3:initialValue});"],
    "description": "Create a nixState"
  },
  "nixStore": {
    "prefix": "nstore",
    "body": ["const ${1:store} = nixStore({", "  ${2:key}: ${3:value}", "});"],
    "description": "Create a nixStore"
  },
  "Event Handler": {
    "prefix": "r-click",
    "body": ["r-click={() => { $1 }}"],
    "description": "Add click event handler"
  },
  
